#!/bin/sh

app_root=$(cd $(dirname "${0}")/.. && pwd)

ln -sn ../../assets/wp-content/themes "${app_root}/public/wp-content/my-themes" || :
ln -sn ../../assets/wp-content/mu-plugins "${app_root}/public/wp-content/mu-plugins" || :
ln -sn ../../assets/wp-content/advanced-cache.php "${app_root}/public/wp-content/advanced-cache.php" || :
